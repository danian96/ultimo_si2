
<div class="card card-outline-info">
  <div class="card-header">
    <h4 class="m-b-0 text-white">Habilidad Persona</h4>
  </div>
  <div class="card-body">
    <div class="form-body">
      <h3 class="box-title">Datos</h3>
      <hr class="m-t-0 m-b-40">
      <p>
        <strong>Nivel:</strong>
        <%= @person_skill.nivel %>
      </p>

      <p>
        <strong>Person:</strong>
        <%= @person_skill.person_id %>
      </p>

      <p>
        <strong>Skill:</strong>
        <%= @person_skill.skill_id %>
      </p>


      <p>
        <%= link_to 'Editar', edit_person_skill_path(@person_skill), class:"btn btn-info"  %>
        <%= link_to 'Atras', person_skills_path, class:"btn btn-info"  %>
      </p>
    </div>
  </div>
</div>
