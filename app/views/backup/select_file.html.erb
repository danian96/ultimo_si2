<div class="card card-outline-info">
  <div class="card-header">
    <h4 class="m-b-0 text-white">Backup</h4>
  </div>
  <div class="card-body">
    <div class="form-body">
      <h5 class="box-title">Restaurar base de Datos</h5>
      <hr class="m-t-0 m-b-40">
      <%= form_with url: backup_restore_path , local: true, html: {class: "form-horizontal"} do |form| %>
        <%= csrf_meta_tags %>
        <div class="col-md-6">
          <div class="form-group row">
            <%= form.file_field :archive %>
          </div>
        </div>
        <div class="form-actions">
          <div class="row">
            <div class="col-md-6">
              <div class="row">
                <div class="col-md-offset-3 col-md-9">
                  <%= form.submit "Guardar", class:"btn btn-info" %>
                  <%= link_to 'Atras',grade_academics_path , class:"btn btn-info"%>
                </div>
              </div>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
